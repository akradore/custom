<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_stock_production" model="ir.sequence">
            <field name="name">Stock Inter Transfer</field>
            <field name="code">stock.inter.picking</field>
            <field name="prefix">INT%(year)s%(month)s%(day)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="location_inter_transit" model="stock.location">
            <field name="name">在途</field>
            <field name="location_id" ref="stock.stock_location_locations"/>
            <field name="usage">transit</field>
            <field name="company_id"></field>
        </record>

    </data>
</odoo>